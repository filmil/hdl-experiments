+@MAINTAINER_MODE_TRUE@	  echo "EXTRA_DIST += \\" ; \
+@MAINTAINER_MODE_TRUE@	  cd $(top_srcdir) ; \
+@MAINTAINER_MODE_TRUE@	  git ls-files -- 'test/*.vhd' 'test/*.txt' 'test/*.sh' 'test/*.dump' \
